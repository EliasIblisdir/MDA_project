    "import pycountry\n",
    "\n",
    "# Define a function to convert ISO2 code to full country name\n",
    "def iso2_to_country_name(iso2_code):\n",
    "    try:\n",
    "        return pycountry.countries.get(alpha_2=iso2_code).name\n",
    "    except:\n",
    "        return None  # or iso2_code if you want to keep the code when not found\n",
    "\n",
    "# Apply the function to your dataframe\n",
    "projects_enriched['country_full'] = projects_enriched['coordinator_country'].apply(iso2_to_country_name)\n"
